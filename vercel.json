{
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/((?!api|css|js|.*\\.png|.*\\.jpg|.*\\.jpeg|.*\\.gif|.*\\.svg|.*\\.ico|.*\\.webp|.*\\.xml|.*\\.txt|.*\\.json).*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "api/contact.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  }
}